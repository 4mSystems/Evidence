<?xml version="1.0" encoding="utf-8"?>

                                                        

    

                 
                                
                                                                                  
           

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp4h"
        app:cardCornerRadius="@dimen/dp6h"
        app:cardElevation="4dp"
        android:tag="layout/item_report_0"                  
        app:cardUseCompatPadding="true"  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp6h">

            <te.app.evidence.customViews.views.CustomTextViewMedium
                android:id="@+id/client_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp15w"
                android:layout_marginTop="@dimen/dp12h"
                android:layout_marginEnd="@dimen/dp4w"
                android:text="@string/clients"
                android:textAlignment="viewStart"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/colorPrimary"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <te.app.evidence.customViews.views.CustomTextViewRegular
                android:id="@+id/client_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp10w"
                android:layout_marginEnd="@dimen/dp20w"
                android:tag="binding_1"                           
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/header_data_table"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/session_date_text"
                app:layout_constraintTop_toTopOf="@+id/client_text" />

            <View
                android:id="@+id/v_client"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/dp12h"
                android:background="@color/line_color"
                app:layout_constraintTop_toBottomOf="@id/client_value" />

            <te.app.evidence.customViews.views.CustomTextViewMedium
                android:id="@+id/opponents_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp15w"
                android:layout_marginTop="@dimen/dp12h"
                android:layout_marginEnd="@dimen/dp4w"
                android:text="@string/opponents"
                android:textAlignment="viewStart"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/colorPrimary"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/v_client" />

            <te.app.evidence.customViews.views.CustomTextViewRegular
                android:id="@+id/opponents_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp10w"
                android:layout_marginEnd="@dimen/dp20w"
                android:tag="binding_2"                          
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/header_data_table"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/session_date_text"
                app:layout_constraintTop_toTopOf="@+id/opponents_text" />

            <View
                android:id="@+id/v_opponents"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/dp12h"
                android:background="@color/line_color"
                app:layout_constraintTop_toBottomOf="@id/opponents_value" />

            <te.app.evidence.customViews.views.CustomTextViewMedium
                android:id="@+id/tv_invetationNum_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp15w"
                android:layout_marginTop="@dimen/dp12h"
                android:layout_marginEnd="@dimen/dp4w"
                android:text="@string/case_number"
                android:textAlignment="viewStart"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/colorPrimary"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/v_opponents" />

            <te.app.evidence.customViews.views.CustomTextViewRegular
                android:id="@+id/tv_invetationNum"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp10w"
                android:layout_marginEnd="@dimen/dp10w"
                android:tag="binding_3"                                        
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/header_data_table"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/tv_invetationNum_text"
                app:layout_constraintTop_toTopOf="@+id/tv_invetationNum_text" />

            <View
                android:id="@+id/v1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/dp8h"
                android:background="@color/line_color"
                app:layout_constraintTop_toBottomOf="@id/tv_invetationNum" />

            <te.app.evidence.customViews.views.CustomTextViewMedium
                android:id="@+id/court_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp15w"
                android:layout_marginTop="@dimen/dp12h"
                android:layout_marginEnd="@dimen/dp4w"
                android:text="@string/inventation_type"
                android:textAlignment="viewStart"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/colorPrimary"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/v1" />

            <te.app.evidence.customViews.views.CustomTextViewRegular
                android:id="@+id/court_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp10w"
                android:layout_marginEnd="@dimen/dp10w"
                android:tag="binding_4"                                          
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/header_data_table"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/court_text"
                app:layout_constraintTop_toTopOf="@+id/court_text" />

            <View
                android:id="@+id/v2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/dp12h"
                android:background="@color/line_color"
                app:layout_constraintTop_toBottomOf="@id/court_value" />

            <te.app.evidence.customViews.views.CustomTextViewMedium
                android:id="@+id/paper_type_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp15w"
                android:layout_marginTop="@dimen/dp12h"
                android:layout_marginEnd="@dimen/dp4w"
                android:text="@string/circle_num"
                android:textAlignment="viewStart"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/colorPrimary"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/v2" />

            <te.app.evidence.customViews.views.CustomTextViewRegular
                android:id="@+id/paper_type_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp10w"
                android:layout_marginEnd="@dimen/dp10w"
                android:tag="binding_5"                                    
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/header_data_table"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/paper_type_text"
                app:layout_constraintTop_toTopOf="@+id/paper_type_text" />

            <View
                android:id="@+id/v3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/dp12h"
                android:background="@color/line_color"
                app:layout_constraintTop_toBottomOf="@id/paper_type_value" />

            <te.app.evidence.customViews.views.CustomTextViewMedium
                android:id="@+id/paper_number_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp15w"
                android:layout_marginTop="@dimen/dp12h"
                android:layout_marginEnd="@dimen/dp4w"
                android:text="@string/court"
                android:textAlignment="viewStart"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/colorPrimary"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/v3" />

            <te.app.evidence.customViews.views.CustomTextViewRegular
                android:id="@+id/paper_number_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp10w"
                android:layout_marginEnd="@dimen/dp10w"
                android:tag="binding_6"                                
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/header_data_table"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/paper_type_text"
                app:layout_constraintTop_toTopOf="@+id/paper_number_text" />

            <View
                android:id="@+id/v6"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/dp12h"
                android:background="@color/line_color"
                app:layout_constraintTop_toBottomOf="@id/paper_number_value" />

            <te.app.evidence.customViews.views.CustomTextViewMedium
                android:id="@+id/session_date_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp15w"
                android:layout_marginTop="@dimen/dp12h"
                android:layout_marginEnd="@dimen/dp4w"
                android:text="@string/session_date"
                android:textAlignment="viewStart"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/colorPrimary"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/v6" />

            <te.app.evidence.customViews.views.CustomTextViewRegular
                android:id="@+id/session_month_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp10w"
                android:layout_marginEnd="@dimen/dp10w"
                android:tag="binding_7"                                
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/header_data_table"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/session_date_text"
                app:layout_constraintTop_toTopOf="@+id/session_date_text" />

            <View
                android:id="@+id/v7"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/dp12h"
                android:background="@color/line_color"
                app:layout_constraintTop_toBottomOf="@id/session_month_value" />

            <te.app.evidence.customViews.views.CustomTextViewMedium
                android:id="@+id/status_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp15w"
                android:layout_marginTop="@dimen/dp12h"
                android:layout_marginEnd="@dimen/dp4w"
                android:text="@string/client_note"
                android:textAlignment="viewStart"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/colorPrimary"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/v7" />

            <te.app.evidence.customViews.views.CustomTextViewRegular
                android:id="@+id/status_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp10w"
                android:layout_marginEnd="@dimen/dp10w"
                android:layout_marginBottom="@dimen/dp6h"
                android:tag="binding_8"                                     
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="@dimen/sp10"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/status_text"
                app:layout_constraintTop_toTopOf="@+id/status_text" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
         