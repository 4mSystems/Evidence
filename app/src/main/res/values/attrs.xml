<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomEditText">
        <attr name="isValidate" format="integer" />
        <attr name="android:inputType" />
        <attr name="android:text" />
        <attr name="drawable" format="reference" />
        <attr name="focusable" format="boolean" />
        <attr name="gravity" />
    </declare-styleable>
    <attr name="gravity" format="enum">
        <enum name="TOP" value="48" />
    </attr>
    <declare-styleable name="FitChart">
        <attr name="strokeSize" format="dimension|reference" />
        <attr name="valueStrokeColor" format="color|reference" />
        <attr name="backStrokeColor" format="color|reference" />
        <attr name="animationMode" />
    </declare-styleable>
<!--    <attr name="animationMode">-->
<!--        <enum name="linear" value="0" />-->
<!--&lt;!&ndash;        <enum name="overdraw" value="1" />&ndash;&gt;-->
<!--    </attr>-->
</resources>
